{
  "name": "stephanmiller/zen-notebook-server",
  "description": "Server for zen notebook",
  "license": "gpl",
  "authors": [
    {
      "name": "Stephan Miller",
      "email": "stephanmil@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "keywords": [
    "editor",
    "writing"
  ],
  "type": "project",
  "require": {
    "laravel/framework": "5.0.*",
    "jlapp/swaggervel": "dev-master",
    "doctrine/dbal": "~2.3",
    "guzzlehttp/guzzle": "~4.0",
    "league/fractal": "0.11.*@dev",
    "janhenkgerritsen/codeception-laravel5": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "league/factory-muffin": "3.0.*@dev",
    "league/oauth2-client": "0.7.*@dev"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ZenNotebook\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}

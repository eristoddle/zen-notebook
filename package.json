{
  "name": "zen-notebook",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start": "expo start --dev-client",
    "npx:android": "npx react-native run-android",
    "npx:ios": "npx react-native run-ios",
    "electron": "yarn expo-electron start",
    "electron:compile": "yarn electron-webpack",
    "electron:pack": "yarn electron-builder --dir",
    "electron:dist": "yarn electron-webpack && yarn electron-builder",
    "electron:postinstall": "electron-builder install-app-deps",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.0",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@reduxjs/toolkit": "^1.7.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-polyfill-corejs2": "^0.3.0",
    "babel-plugin-polyfill-corejs3": "^0.4.0",
    "babel-plugin-polyfill-regenerator": "^0.3.0",
    "expo": "~41.0.1",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.5.4",
    "html-webpack-plugin": "^4.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-electron": "^0.14.0",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-localization": "^2.1.7",
    "react-native-localize": "^2.1.7",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-unimodules": "^0.13.3",
    "react-native-web": "~0.13.12",
    "react-redux": "^7.2.6",
    "simple-markdown": "^0.7.3",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.12.71",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "electron": "^6.0.12",
    "electron-builder": "^22.14.5",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "patch-package": "^6.4.7",
    "prettier": "2.4.1"
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "resolutions": {
    "js-yaml": "^3.13.1",
    "html-loader": "1.0.0-alpha.0"
  },
  "private": true
}
